


docker run -v `pwd`:/app  -v /tmp/.X11-unix:/tmp/.X11-unix  -e DISPLAY=$DISPLAY   -it osrf/ros:humble-desktop
xhost local:root

export ROS_DOMAIN_ID=1

colcon build --symlink-install

ros2 pkg create --build-type ament_python --license MIT   bot
colcon build --symlink-install
source  install/local_setup.bash
ros2 run bot talker

ros2 topic pub /cmd_vel geometry_msgs/Twist  "linear: { x: 0.1 }"
ros2 launch bot simulate.launch.py

ros2 run teleop_twist_keyboard teleop_twist_keyboard

docker run -it --name apache-server -p 8080:80 -v `pwd`/share:/usr/local/apache2/htdocs/ httpd:2.4
